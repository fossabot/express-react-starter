{
  "name": "express-react-starter",
  "version": "0.0.1-alpha",
  "description": "A react + express starter",
  "main": "index.js",
  "scripts": {
    "run:frontend": "tsc scripts/server.ts && node scripts/server.js",
    "dev:frontend": "webpack-dev-server --hot-only --host 0.0.0.0 --no-inline --config frontend/webpack.development.config.js",
    "build:frontend": "webpack --config frontend/webpack.production.config.js",
    "trans:compile": "tsc -p . --target ES6 --module es6 --jsx preserve --outDir extracted",
    "trans:extract": "babel --config-file=scripts/options/.babelrc \"extracted/**/*.jsx\"",
    "trans:manage": "tsc scripts/translation.ts && node scripts/translation.js",
    "translate:frontend": "npm run trans:compile && npm run trans:extract && npm run trans:manage",

    "run:backend": "NODE_PATH=./backend/src node ./backend/scripts/backend.js",
    "build:backend": "cd backend && tsc",
    "tslint": "tslint --project frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SidiaDevelopment/express-react-starter.git"
  },
  "author": "Marvin Fischer <sidiastudios@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SidiaDevelopment/express-react-starter/issues"
  },
  "homepage": "https://github.com/SidiaDevelopment/express-react-starter#readme",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/mysql": "^2.15.5",
    "@types/prop-types": "^15.5.6",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-intl": "^2.3.11",
    "@types/webpack-env": "^1.13.6",
    "antd": "^3.10.1",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-react-intl": "^3.0.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "css-loader": "^1.0.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "js-sha256": "^0.9.0",
    "mini-css-extract-plugin": "^0.4.4",
    "mobx": "^5.5.1",
    "mobx-react": "^5.3.4",
    "mysql": "^2.16.0",
    "node-sass": "^4.9.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-intl": "^2.7.1",
    "react-intl-translations-manager": "^5.0.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.2.2",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.1.3",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}
